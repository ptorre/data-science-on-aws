        [1;34m"summary"[0m: [32m"A Kerberos authentication handler for python-requests"[0m,
        [1;34m"home_page"[0m: [32m"https://github.com/requests/requests-kerberos"[0m,
        [1;34m"author"[0m: [32m"Ian Cordasco, Cory Benfield, Michael Komitee"[0m,
        [1;34m"author_email"[0m: [32m"graffatcolmingov@gmail.com"[0m,
