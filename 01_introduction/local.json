        [1;34m"classifier"[0m: [1m[[0m
          [32m"License :: OSI Approved :: ISC License (ISCL)"[0m
        [1m][0m,
        [1;34m"requires_dist"[0m: [1m[[0m
          [32m"requests (>=1.1.0)"[0m,
          [32m"cryptography (>=1.3)"[0m,
